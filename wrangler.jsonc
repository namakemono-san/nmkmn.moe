{
	"$schema": "node_modules/wrangler/config-schema.json",
	"name": "nmkmn-moe",
	"compatibility_date": "2026-02-16",
	"main": "worker/index.ts",
	"assets": {
		"directory": "./dist",
		"binding": "ASSETS",
		"not_found_handling": "single-page-application",
		"run_worker_first": [
			"/api/*"
		]
	},
	"kv_namespaces": [
		{
			"binding": "VIEWS",
			"id": "fdb9725ebe8841b89f085894aca5b480",
			"remote": true
		}
	]
}